{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0|^8.2",
        "darkaonline/l5-swagger": "8.5.0",
        "doctrine/dbal": "3.6.2",
        "fruitcake/laravel-cors": "dev-develop",
        "guzzlehttp/guzzle": "^7.5",
        "korridor/laravel-has-many-sync": "^2.0",
        "laravel-validation-rules/phone": "^1.8",
        "laravel/framework": "^10.10.1",
        "laravel/passport": "v11.8.4",
        "laravel/scout": "^10.2",
        "laravel/socialite": "^5.6",
        "laravel/tinker": "^2.8",
        "laravel/ui": "^4.2",
        "league/csv": "^9.9",
        "lorisleiva/laravel-actions": "^2.5",
        "nesbot/carbon": "^2.72",
        "nick322/secure-spreadsheet": "^1.0",
        "openai-php/client": "^0.6.3",
        "orangehill/iseed": "^3.0",
        "owen-it/laravel-auditing": "v13.5.0",
        "php-parallel-lint/php-parallel-lint": "^1.3",
        "phpoffice/phpspreadsheet": "^1.28",
        "spatie/laravel-json-api-paginate": "^1.13",
        "spatie/laravel-permission": "5.10.1",
        "spatie/laravel-query-builder": "^5.2",
        "zircote/swagger-php": "4.7.10"
    },
    "require-dev": {
        "fakerphp/faker": "v1.21.0",
        "friendsofphp/php-cs-fixer": "v3.16.0",
        "jubeki/laravel-code-style": "2.4.0",
        "knuckleswtf/scribe": "^4.21",
        "mockery/mockery": "1.5.1",
        "nunomaduro/collision": "v7.5.2",
        "pestphp/pest-plugin-faker": "v2.0.0",
        "pestphp/pest-plugin-laravel": "v2.0.0",
        "php-parallel-lint/php-console-highlighter": "v1.0.0",
        "phpunit/phpunit": "10.1.3",
        "squizlabs/php_codesniffer": "3.7.2",
        "vanilla/garden-cli": "v4.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "check-style": "php-cs-fixer fix --dry-run --diff",
        "fix-style": "php-cs-fixer fix .",
        "format": "php-cs-fixer fix ."
    }
}
